<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>ca.wilkinsonlab.sadi</groupId>
  <artifactId>sadi-registry</artifactId>
  <packaging>war</packaging>
  <version>0.0.6-SNAPSHOT</version>
  <name>sadi-registry Maven Webapp</name>
  <url>http://sadiframework.org/registry/</url>
  <parent>
    <groupId>ca.wilkinsonlab.sadi</groupId>
    <artifactId>sadi-webapp</artifactId>
    <version>0.0.2-SNAPSHOT</version>
  </parent>
  <build>
    <finalName>sadi-registry</finalName>
  </build>
  <repositories>
    <repository>
  		<id>dev.biordf.net</id>
  		<name>dev.biordf.net</name>
  		<url>http://dev.biordf.net/maven</url>
  		<snapshots>
  			<enabled>true</enabled>
  		</snapshots>
  	</repository>
    <repository>
        <id>twitter4j.org</id>
        <name>twitter4j.org Repository</name>
        <url>http://twitter4j.org/maven2</url>
        <releases>
            <enabled>true</enabled>
        </releases>
        <snapshots>
            <enabled>true</enabled>
        </snapshots>
     </repository>
  </repositories>
  <dependencies>
  
    <dependency>
      <groupId>ca.wilkinsonlab.sadi</groupId>
      <artifactId>sadi-client</artifactId>
      <version>0.0.5-SNAPSHOT</version>
    </dependency>
    
    <dependency>
      <groupId>com.openlinksw.virtuoso</groupId>
      <artifactId>virt_jena</artifactId>
      <version>5.0.11.3039.Jena2.6-SNAPSHOT</version>
      <!-- 
      	temporarily exclude Jena until we can rebuild virt_jena against
      	Jena 2.6.3
      -->
  	  <exclusions>
	    <exclusion>
	      <groupId>com.hp.hpl.jena</groupId>
	      <artifactId>jena</artifactId>
	    </exclusion>
      </exclusions>
    </dependency>
    
    <dependency> <!-- required by service.jsp for pageContext -->
      <groupId>javax.servlet</groupId>
      <artifactId>jsp-api</artifactId>
      <version>2.0</version>
    </dependency>
    
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>jstl</artifactId>
      <version>1.1.2</version>
    </dependency>
    
    <dependency>
      <groupId>taglibs</groupId>
      <artifactId>standard</artifactId>
      <version>1.1.2</version>
    </dependency>
    
    <dependency>
      <groupId>stringtree</groupId>
      <artifactId>stringtree-json</artifactId>
      <version>2.0.9</version>
    </dependency>
    
    <dependency>
      <groupId>org.twitter4j</groupId>
      <artifactId>twitter4j-core</artifactId>
      <version>2.1.3</version>
    </dependency>
    
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>
