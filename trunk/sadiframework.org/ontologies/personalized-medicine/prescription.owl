<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY sio "http://semanticscience.org/resource/" >
    <!ENTITY lsrn "http://purl.oclc.org/SADI/LSRN/" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>

<rdf:RDF
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:sio="http://semanticscience.org/resource/"
     xmlns:lsrn="http://purl.oclc.org/SADI/LSRN/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     >

    <owl:Ontology rdf:about=""/>

    <owl:ObjectProperty rdf:about="#prescription"/> 
    <owl:ObjectProperty rdf:about="#dosage-form"/>

    <owl:Class rdf:about="#Prescription"/>
    <owl:Class rdf:about="#PrescribedDrug"/>
    <owl:Class rdf:about="#Dose"/>

    <owl:Class rdf:about="#MeasurementUnit"/>
    <owl:Class rdf:about="#Milligrams">
       <rdfs:subClassOf rdf:resource="#MeasurementUnit"/>
    </owl:Class> 

    <owl:Class rdf:about="#DosageForm"/>
    <owl:Class rdf:about="#Pill">
        <rdfs:subClassOf rdf:resource="#DosageForm"/>
    </owl:Class> 

</rdf:RDF>

