<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY LSRN "http://purl.oclc.org/SADI/LSRN/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY ontology "http://www.mygrid.org.uk/ontology#" >
    <!ENTITY resource "http://semanticscience.org/resource/" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY properties "http://sadiframework.org/ontologies/properties.owl#" >
]>

<rdf:RDF xmlns="http://dev.biordf.net/~kawas/sawsdl/owl/get_drugs_by_pathway.owl#"
     xml:base="http://dev.biordf.net/~kawas/sawsdl/owl/get_drugs_by_pathway.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:LSRN="http://purl.oclc.org/SADI/LSRN/"
     xmlns:resource="http://semanticscience.org/resource/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:properties="http://sadiframework.org/ontologies/properties.owl#"
     xmlns:ontology="http://www.mygrid.org.uk/ontology#">
    <owl:Ontology rdf:about="http://dev.biordf.net/~kawas/sawsdl/owl/get_drugs_by_pathway.owl">
        <owl:imports rdf:resource="http://semanticscience.org/resource/SIO_000132"/>
        <owl:imports rdf:resource="http://purl.oclc.org/SADI/LSRN/"/>
    </owl:Ontology>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://semanticscience.org/resource/SIO_000132 -->

    <owl:ObjectProperty rdf:about="&resource;SIO_000132"/>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <!-- http://dev.biordf.net/~kawas/sawsdl/owl/get_drugs_by_pathway.owl#getDrugRecordsByPathway -->

    <owl:Class rdf:about="http://dev.biordf.net/~kawas/sawsdl/owl/get_drugs_by_pathway.owl#getDrugRecordsByPathway">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&LSRN;KEGG_PATHWAY_Record"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&resource;SIO_000132"/>
                        <owl:someValuesFrom rdf:resource="&LSRN;KEGG_DRUG_Record"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>


    <!-- http://purl.oclc.org/SADI/LSRN/KEGG_DRUG_Record -->

    <owl:Class rdf:about="&LSRN;KEGG_DRUG_Record"/>
    
    <!-- http://purl.oclc.org/SADI/LSRN/KEGG_PATHWAY_Record -->

    <owl:Class rdf:about="&LSRN;KEGG_PATHWAY_Record"/>   

</rdf:RDF>