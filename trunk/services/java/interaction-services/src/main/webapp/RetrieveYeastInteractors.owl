<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY lsrn "http://purl.oclc.org/SADI/LSRN/">
  <!ENTITY interaction "http://sadiframework.org/interaction-services/interaction.owl">
]>
<rdf:RDF
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:owl="&owl;"
    xmlns:lsrn="&lsrn;"
    xmlns:interaction="&interaction;"
    >

  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="&interaction"/>
  </owl:Ontology>

  <owl:Class rdf:ID="OutputClass">
    <rdfs:subClassOf rdf:resource="&lsrn;SGD_Record"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&interaction;#interacts_with"/>
        <owl:someValuesFrom rdf:resource="&lsrn;SGD_Record"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

</rdf:RDF>