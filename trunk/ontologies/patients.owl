<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY regress "http://sadiframework.org/examples/regression.owl#">
]>

<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:regress="&regress;"
    xmlns="http://sadiframework.org/ontologies/patients.owl#"
    xml:base="http://sadiframework.org/ontologies/patients.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="&regress;"/>
  </owl:Ontology>

  <owl:Class rdf:ID="Patient"/>

  <owl:ObjectProperty rdf:ID="creatinineLevels">
    <rdfs:domain rdf:resource="#Patient"/>
    <rdfs:range rdf:resource="#Collection"/>
  </owl:ObjectProperty>
  
  <owl:DatatypeProperty rdf:ID="dateOffset">
    <rdfs:subPropertyOf rdf:resource="&regress;x"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:ID="creatinineLevel">
    <rdfs:subPropertyOf rdf:resource="&regress;y"/>
  </owl:DatatypeProperty>

  <owl:Class rdf:ID="ElevatedCreatininePatient">
    <rdfs:subClassOf rdf:resource="#Patient"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#creatinineLevels"/>
        <owl:someValuesFrom rdf:resource="#ElevatedCreatinineCollection"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>

  <owl:Class rdf:ID="ElevatedCreatinineCollection">
    <rdfs:subClassOf rdf:resource="&regress;Collection"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&regress;hasRegressionModel"/>
        <owl:someValuesFrom rdf:resource="&regress;IncreasingLinearRegressionModel"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  
</rdf:RDF>
