<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY SO "http://purl.org/obo/owl/SO#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY sio "http://semanticscience.org/resource/" >
    <!ENTITY lsrn "http://purl.oclc.org/SADI/LSRN/" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY properties "http://sadiframework.org/ontologies/properties.owl#" >
]>

<rdf:RDF
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:sio="http://semanticscience.org/resource/"
     xmlns:lsrn="http://purl.oclc.org/SADI/LSRN/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:properties="http://sadiframework.org/ontologies/properties.owl#"
     xmlns:SO="http://purl.org/obo/owl/SO#">

    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="http://purl.org/obo/owl/sequence"/>
        <owl:imports rdf:resource="http://sadiframework.org/ontologies/properties.owl"/>
    </owl:Ontology>

   <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="#version">
        <rdfs:subPropertyOf rdf:resource="&sio;SIO_000008"/> <!-- 'has attribute' --> 
    </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="#Assembly">
        <rdfs:subClassOf rdf:resource="&SO;SO_0001248"/> <!-- 'assembly' -->
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&sio;SIO_000066"/>  <!-- 'has source' -->
                        <owl:someValuesFrom rdf:resource="#Organism"/> <!-- 'identifier' -->
                    </owl:Restriction>
                    <owl:Restriction>
                        <!-- version number for sequence assembly (optional) -->
                        <owl:onProperty rdf:resource="#version"/>  
                        <owl:minCardinality>0</owl:minCardinality>
                    </owl:Restriction>
                 </owl:intersectionOf>
            </owl:Class>
         </owl:equivalentClass>
    </owl:Class>

    <owl:Class rdf:about="#Organism">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sio;SIO_000008"/>  <!-- 'has attribute' -->
                <owl:someValuesFrom rdf:resource="&lsrn;taxon_Identifier"/>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>

</rdf:RDF>

